{
	"name": "node-regon",
	"version": "0.1.51",
	"description": "A library for accessing data about businesses from the National Statistics Office in Poland. It can be used to access data stored in the National Court register using unique identifiers that each company has such as NIP (Tax Identification) and REGON (Statistical) numbers.",
	"main": "index.js",
	"scripts": {
		"test": "mocha --timeout 10000 test/*-test.js"
	},
	"options": {
		"mocha": "--require .scripts/mocha_runner client/**/tests/**/*.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Appsolutly/node-regon.git"
	},
	"keywords": [
		"regon",
		"node-regon",
		"antigate",
		"anti-captcha",
		"nip",
		"krs",
		"wyszukiwarkaregon",
		"wyszukiwarkaregontest",
		"wsdl",
		"gus",
		"stat.gov.pl",
		"identification number"
	],
	"author": "Adam Kaczmarzyk",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Appsolutly/node-regon/issues"
	},
	"homepage": "https://github.com/Appsolutly/node-regon#readme",
	"devDependencies": {
		"babel-core": "^6.7.4",
		"babel-eslint": "^6.1.0",
		"babel-polyfill": "^6.7.4",
		"babel-preset-es2015": "6.x.x",
		"babel-preset-stage-2": "6.x.x",
		"chai": "3.x.x",
		"enzyme": "1.x.x",
		"eslint": "1.10.x",
		"mocha": "2.x.x"
	},
	"dependencies": {
		"antigate": "0.0.1",
		"chai": "^3.5.0",
		"deasync": "^0.1.6",
		"debug": "^2.2.0",
		"lodash": "^4.12.0",
		"soap-extended": "^0.25.0",
		"xml2js": "^0.4.19"
	}
}
